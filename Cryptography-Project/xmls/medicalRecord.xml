                        </hair>
                        <nails>
                            <condition></condition>
                        </nails>
                        <mucousMembranes>
                            <condition></condition>
                        </mucousMembranes>
                    </skinExam>
                    <chest>
                        <condition></condition>
                    </chest>
                    <urinary>
                        <condition></condition>
                    </urinary>
                    <bonesAndJoints>
                        <condition></condition>
                    </bonesAndJoints>
                </section>
            </component>
