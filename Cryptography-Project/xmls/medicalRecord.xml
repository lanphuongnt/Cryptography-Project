                    </personal>
                    <familyHistory>
                        <value code=""/>
                        <additionalText></additionalText>
                    </familyHistory>
                </section>
            </component>
        </structuredBody>
    </component>

    <!-- Physical Examination Section -->
    <component>
        <structuredBody>
            <component>
                <section>
                    <templateId root="2.16.840.1.113883.10.20.22.2.5.1"/>
                    <code code="48765-2" codeSystem="2.16.840.1.113883.6.1"/>
