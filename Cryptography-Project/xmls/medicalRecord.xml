    <id root="2.16.840.1.113883.19.5" extension="123456"/>

    <!-- Clinical Document Title -->
    <title>Medical Record</title>

    <!-- Patient Information Section -->
    <recordTarget>
        <patientRole>
            <id root="2.16.840.1.113883.19.5" extension="PatientID"/>
            <patient>
                <name>
                    <given></given>
                    <family></family>
                </name>
                <administrativeGenderCode code=""/>
                <birthTime value=""/>
                <addr>
                    <streetAddressLine></streetAddressLine>
                    <city></city>
                    <state></state>
                    <postalCode></postalCode>
                </addr>
                <telecom use="HP">
                    <value></value>
                </telecom>
            </patient>
        </patientRole>
    </recordTarget>

    <!-- Encounter Details Section -->
    <component>
        <structuredBody>
            <component>
                <section>
                    <templateId root="2.16.840.1.113883.10.20.22.2.22.1"/>
                    <code code="46240-8" codeSystem="2.16.840.1.113883.6.1"/>
