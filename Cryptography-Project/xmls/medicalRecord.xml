                    <diagnosticTests>
                        <test></test>
                    </diagnosticTests>
                    <staging>
                        <stage></stage>
                    </staging>
                    <treatmentPlan>
                        <treatmentPlanDescription>
                            <description></description>
                        </treatmentPlanDescription>
                    </treatmentPlan>
                    <followUpPlan>
                        <followUpAppointmentDate></followUpAppointmentDate>
                        <prognosis></prognosis>
                    </followUpPlan>
                </section>
            </component>
        </structuredBody>
