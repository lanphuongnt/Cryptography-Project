                        <pulse>
                            <BPM></BPM>
                        </pulse>
                        <bloodPressure>
                            <systolic></systolic>
                            <diastolic></diastolic>
                        </bloodPressure>
                        <respiratoryRate></respiratoryRate>
                        <temperature></temperature>
