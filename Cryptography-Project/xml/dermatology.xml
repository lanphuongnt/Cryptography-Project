<ClinicalDocument xmlns="urn:hl7-org:v3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <!-- Header Section -->
    <realmCode code="US"/>
    <typeId root="2.16.840.1.113883.1.3" extension="POCD_HD000040"/>
    <templateId root="2.16.840.1.113883.10.20.22.1.1"/>
    <id root="2.16.840.1.113883.19.5" extension="123456"/>

    <!-- Clinical Document Title -->
    <title>Medical Record</title>

    <!-- Patient Information Section -->
    <recordTarget>
        <patientRole>
            <id root="2.16.840.1.113883.19.5" extension="PatientID"/>
            <patient>
                <name>
                    <given></given>
                    <family></family>
                </name>
                <administrativeGenderCode code=""/>
                <birthTime value="YYYYMMDD"/>
                <addr>
                    <streetAddressLine></streetAddressLine>
                    <city></city>
                    <state></state>
                    <postalCode></postalCode>
                </addr>
                <telecom use="HP">
                    <value></value>
                </telecom>
            </patient>
        </patientRole>
    </recordTarget>

    <!-- Medical History Section -->
    <component>
        <structuredBody>
            <component>
                <section>
                    <templateId root="2.16.840.1.113883.10.20.22.2.14.1"/>
                    <code code="11348-0" codeSystem="2.16.840.1.113883.6.1"/>
                    <title>Medical History</title>
                    <!-- Include relevant medical history details here -->
                    <presentingComplaint>
                        <complaint></complaint>
                    </presentingComplaint>
                    <clinicalFindings>
                        <findings></findings>
                    </clinicalFindings>
                    <allergenTesting>
                        <testing></testing>
                    </allergenTesting>
                    <diagnosis>
                        <diagnosisDescription>
                            <description></description>
                        </diagnosisDescription>
                    </diagnosis>
                    <treatmentPlan>
                        <allergenAvoidance></allergenAvoidance>
                        <topicalSteroids></topicalSteroids>
                        <antihistamines></antihistamines>
                    </treatmentPlan>
                    <patientEducation></patientEducation>
                    <followUpPlan>
                        <followUpAppointmentDate></followUpAppointmentDate>
                        <prognosis></prognosis>
                    </followUpPlan>
                </section>
            </component>
        </structuredBody>
    </component>

    <!-- Diagnosis Section -->
    <component>
        <structuredBody>
            <component>
                <section>
                    <templateId root="2.16.840.1.113883.10.20.22.2.8.1"/>
                    <code code="11450-4" codeSystem="2.16.840.1.113883.6.1"/>
                    <title>Diagnosis</title>
                    <!-- Include relevant diagnosis details here -->
                    <diagnosis>
                        <diagnosisDescription>
                            <description></description>
                        </diagnosisDescription>
                        <diagnosisDate></diagnosisDate>
