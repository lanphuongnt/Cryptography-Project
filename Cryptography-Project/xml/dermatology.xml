        </structuredBody>
    </component>

    <!-- Follow-Up Section -->
    <component>
        <structuredBody>
            <component>
                <section>
                    <templateId root="2.16.840.1.113883.10.20.22.2.23.1"/>
                    <code code="11513-9" codeSystem="2.16.840.1.113883.6.1"/>
