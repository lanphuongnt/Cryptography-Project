    <!-- Staging Section -->
    <component>
        <structuredBody>
            <component>
                <section>
                    <templateId root="2.16.840.1.113883.10.20.22.2.21.1"/>
                    <code code="11512-1" codeSystem="2.16.840.1.113883.6.1"/>
                    <title>Staging</title>
                    <!-- Include relevant staging details here -->
                        <staging>
                            <stagingDate></stagingDate>
